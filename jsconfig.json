{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es6",
    "checkJs": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "baseUrl": "node_modules"
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
